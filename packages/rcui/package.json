{
  "name": "rcui",
  "version": "1.6.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.2.0"
  },
  "scripts": {
    "prestorybook": "npm run test:generate-output",
    "prebuild:storybook": "npm run test:generate-output",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "update-snapshot": "jest --updateSnapshot storyshots.test.jsx",
    "build:storybook": "build-storybook -c .storybook -o public",
    "build:lib": "rollup -c",
    "serve:lib": "rollup -c -w",
    "test:cover": "jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "lint:css": "stylelint './src/**/*.ts ./src/**/*.tsx'",
    "lint:ts": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}'",
    "commit": "git-cz",
    "docs:api": "rimraf ./pages/api && babel-node ./docs/scripts/buildApi.js ./src/components ./pages/api",
    "docs:build": "rimraf .next && NODE_ENV=production next build",
    "docs:build-sw": "babel-node ./docs/scripts/buildServiceWorker.js",
    "docs:dev": "rimraf node_modules/.cache/babel-loader && next dev",
    "docs:export": "rimraf docs/export && next export -o docs/export && yarn docs:build-sw && cp -r docs/static/. docs/export",
    "docs:start": "next start",
    "docs:typescript": "cross-env NODE_ENV=development babel docs/src --config-file ./docs/babel.config.ts.js --extensions .tsx --out-dir docs/src",
    "docs:typescript:formatted": "node docs/scripts/formattedTSDemos",
    "update-icon": "node scripts/update-icon.js",
    "postupdate-icon": " jest src/components/Iconography/__tests__/icons.test.ts --watch"
  },
  "dependencies": {
    "@material-ui/core": "4.2.1",
    "@material-ui/lab": "4.0.0-alpha.18",
    "@material-ui/styles": "4.2.1",
    "deepmerge": "3.2.0",
    "eventemitter2": "5.0.1",
    "lodash": "4.17.11",
    "styled-components": "4.0.3",
    "tinycolor2": "1.4.1"
  },
  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/node": "7.2.2",
    "@storybook/addon-a11y": "5.0.10",
    "@storybook/addon-actions": "5.0.10",
    "@storybook/addon-info": "5.0.10",
    "@storybook/addon-jest": "5.0.10",
    "@storybook/addon-knobs": "5.0.10",
    "@storybook/addon-storyshots": "5.0.10",
    "@storybook/react": "5.0.10",
    "@svgr/rollup": "^4.2.0",
    "@types/enzyme": "3.9.0",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.120",
    "@types/next": "8.0.1",
    "@types/node": "11.11.3",
    "@types/react": "16.8.12",
    "@types/react-dom": "16.8.3",
    "@types/react-test-renderer": "16.8.1",
    "@types/storybook__addon-a11y": "5.0.0",
    "@types/storybook__addon-actions": "3.4.2",
    "@types/storybook__addon-info": "4.1.1",
    "@types/storybook__addon-jest": "3.4.2",
    "@types/storybook__addon-knobs": "5.0.0",
    "@types/storybook__react": "4.0.1",
    "@types/styled-components": "4.0.3",
    "@types/tinycolor2": "1.4.1",
    "awesome-typescript-loader": "5.2.1",
    "axios": "0.19.0-beta.1",
    "babel-plugin-macros": "2.5.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-preval": "2.0.1",
    "babel-plugin-require-context-hook": "1.0.0",
    "chalk": "2.4.2",
    "clipboard-copy": "2.0.1",
    "clsx": "1.0.3",
    "commitizen": "3.0.7",
    "doctrine": "3.0.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.9.1",
    "enzyme-to-json": "3.3.5",
    "fg-loadcss": "2.1.0",
    "git-branch": "2.0.1",
    "husky": "1.3.1",
    "jest": "24.3.1",
    "jest-raw-loader": "1.0.1",
    "jest-styled-components": "6.3.1",
    "jira-connector": "2.10.0",
    "jss-rtl": "0.2.3",
    "lz-string": "1.4.4",
    "marked": "0.6.1",
    "next": "8.0.3",
    "nprogress": "0.2.0",
    "prismjs": "1.15.0",
    "raf": "3.4.1",
    "react-docgen": "4.0.0",
    "react-docgen-typescript-loader": "3.1.0",
    "react-frame-component": "4.0.2",
    "react-jss": "8.6.1",
    "react-redux": "6.0.1",
    "react-swipeable-views": "0.13.1",
    "react-test-renderer": "16.8.3",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "rollup": "^1.12.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "2.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-entry": "2.1.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-url": "^2.2.1",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.2",
    "ts-jest": "24.0.2",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.13.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-react": "3.6.0",
    "typescript": "3.4.2",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "3.0.4",
    "winston": "3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint:ts && yarn lint:css && yarn test:cover",
      "commit-msg": "node scripts/validate-commit-msg.js"
    }
  }
}
