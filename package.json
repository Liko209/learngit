{
  "name": "fiji",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.2.0"
  },
  "scripts": {
    "preinstall": "node scripts/check-pm.js",
    "postinstall": "npm run bt",
    "start:ui": "cd packages/ui-components && npm run storybook",
    "start:app": "cd application && npm run start",
    "clean:build": "rm -rf ./packages/*/build ./packages/*/build-es",
    "clean:all": "lerna clean --yes && rm -rf ./node_modules && npm run clean:build",
    "build:packages": "node ./scripts/build.js",
    "build:app": "cd application && npm run build",
    "build:ui": "cd packages/ui-components && npm run build-storybook",
    "test": "node ./scripts/test.js --env=jsdom",
    "test:staged": "npm run test:cover -- --findRelatedTests",
    "test:cover": "npm run test -- --coverage",
    "test:dev": "npm run test -- --coverage --watchAll",
    "lint": "tslint",
    "lint-all": "tslint -p application && tslint -p packages/foundation && tslint -p packages/sdk && tslint -p packages/ui-components",
    "publish": "npm run clean:build && npm run build && lerna publish --silent",
    "watch": "node scripts/watch.js",
    "prepush": "npm run lint-all && npm run test:cover",
    "commits": "git-cz",
    "commits:all": "git add . && git-cz",
    "commitmsg": "node scripts/validate-commit-msg.js",
    "bt": "lerna bootstrap --hoist --no-ci",
    "raise-coverage": "node scripts/raise-coverage"
  },
  "devDependencies": {
    "@types/enzyme": "*",
    "@types/glob": "*",
    "@types/jest": "*",
    "@types/lodash": "*",
    "@types/node": "*",
    "@types/webpack-env": "*",
    "async": "2.6.1",
    "babel-core": "6.26.3",
    "chalk": "2.4.1",
    "commitizen": "2.10.1",
    "cz-customizable": "git+ssh://git@git.ringcentral.com:Fiji/cz-customizable.git#v0.0.5",
    "dts-generator": "2.1.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "fake-indexeddb": "2.0.4",
    "glob": "7.1.3",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "lerna": "3.3.0",
    "lint-staged": "7.2.2",
    "mkdirp": "0.5.1",
    "raf": "3.4.0",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "resolve": "1.8.1",
    "rimraf": "2.6.2",
    "rollup": "0.65.2",
    "rollup-plugin-commonjs": "9.1.6",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.3.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-tslint": "0.1.35",
    "rollup-plugin-typescript2": "0.17.0",
    "rollup-plugin-uglify": "5.0.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "semver": "5.5.1",
    "shelljs": "0.8.2",
    "string-length": "2.0.0",
    "ts-jest": "23.1.4",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3",
    "uuid": "3.3.2"
  },
  "dependencies": {
    "@types/react-document-title": "2.0.3",
    "react-document-title": "2.0.3"
  }
}
