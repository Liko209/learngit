{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./.tsc_cache/cache_path",
    "baseUrl": ".",
    "outDir": "build",
    "target": "es6",
    "paths": {
      "@/*": ["src/*"],
      "foundation/*": ["../packages/foundation/src/*"],
      "sdk/*": ["../packages/sdk/src/*"],
      "jui/*": ["../packages/jui/src/*"],
      "framework/*": ["../packages/framework/src/*"],
      "shield/*": ["../tests/shield/src/*"],
      "rcui/*": ["../packages/rcui/src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
