{
  "extends": [
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-trailing-underscore",
      "allow-pascal-case",
      "allow-snake-case"
    ],
    "import-name": [
      false
    ]
  },
  "jsRules": {}
}
