{
  "fileheader.tpl": "/*\r\n * @Author: {author}\r\n * @Date: {createTime}\r\n * Copyright Â© RingCentral. All rights reserved.\r\n */\r\n",
  "prettier.tslintIntegration": false,
  "prettier.eslintIntegration": true,
  "editor.formatOnSave": true,
  "typescript.surveys.enabled": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "tslint.exclude": "**/testcafe/**/*.ts",
  "[handlebars]": {
    "editor.formatOnSave": false
  },
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "eslint.autoFixOnSave": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": false },
    { "language": "typescriptreact", "autoFix": false }
  ]
}
