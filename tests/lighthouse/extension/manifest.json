{
    "name": "PerformanceMonitor",
    "description": "performance metric monitor",
    "version": "0.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": true
    },
    "key": "MIIEvQIBADANBgkqhkiG9w0BAQEFAASCBKcwggSjAgEAAoIBAQDTQ4tuP2Kb4S1w",
    "permissions": [
        "tabs",
        "processes"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "js/content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "externally_connectable": {
        "matches": [
            "*://*.gliprc.com/*",
            "*://*.nordigy.ru/*"
        ]
    }
}